# Génération de la clé privée de l'État (RSA 4096 bits)
openssl genrsa -out data/keys/state_private.key 4096

# Génération du certificat d'État (valide 10 ans)
openssl req -new -x509 -sha256 -key data/keys/state_private.key \
    -out data/keys/state_auth.crt \
    -days 3650 \
    -subj "/C=CD/ST=Kinshasa/L=Kinshasa/O=Defense/OU=SIGINT-Sovereign-Core/CN=FARDC-AUTH"
